id: pregame
file_type: screen

import:
  - ../Templates/

  - /YUI/Animations/transitions.yui
  - /YUI/Widgets/

root:
  type: panel
  layout: canvas

  elements:
    - type: panel
      size: { w: 240, h: 270 }
      alignment: { h: center }
      canvas:
        top: 5
        center: h
      background: yui_panel_bg
      padding: 10
      
        elements:
          - type: slider
            value: @audio_volume 
          